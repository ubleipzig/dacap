    "@angular/platform-browser": "^4.3.6",
    "@angular/platform-browser-dynamic": "^4.3.6",
    "@angular/router": "^4.3.6",
    "body-parser": "^1.17.2",
    "compression": "^1.7.0",
    "core-js": "^2.5.0",
    "cors": "^2.8.4",
    "debug": "^3.0.1",
    "express": "^4.15.4",
    "js-object-pretty-print": "^0.3.0",
    "node-cache": "^4.1.5",
    "q": "^1.5.0",
    "request": "^2.81.0",
    "rxjs": "^5.4.3",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/compression": "0.0.33",
    "@types/cors": "^2.8.1",
    "@types/debug": "0.0.30",
    "@types/express": "^4.0.37",
    "@types/mocha": "^2.2.42",
    "@types/nock": "^8.2.1",
