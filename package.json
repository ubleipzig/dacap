    "jquery": "^3.2.1",
    "mocha": "^3.5.0",
    "should": "^11.2.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "ts-mocha": "^1.0.3",
    "tsc-watch": "^1.0.7",
    "typescript": "^2.4.2",
    "webpack": "^3.5.5",
    "webpack-merge": "^4.1.0"
  }
